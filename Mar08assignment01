# count of consonants for given string
def isConsonant(ch):
  ch = ch.upper()
  return not (ch == 'A' or ch == 'E' or
       ch == 'I' or ch == 'O' or
       ch == 'U') and ord(ch) >= 65 and ord(ch) <= 90
def totalConsonants(string):
   count = 0
   for i in range(len(string)):
   if (isConsonant(string[i])):
       count += 1
      return count
  string = "abc de"
print(totalConsonants(string))
